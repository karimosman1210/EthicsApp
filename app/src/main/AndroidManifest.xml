<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.elmostafa.ethicstask">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ethics_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ethics_logo"
        android:supportsRtl="false"
        android:theme="@style/Theme.EthicsTask">
        <activity android:name=".welcome_page.WelcomeActivity"></activity>
        <activity android:name=".login_page.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>